{
  "name": "nestjs-graphql-cursor-connections",
  "version": "1.1.4",
  "description": "GraphQL Cursor Connections specification implementation for NestJS.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azhulin/nestjs-graphql-cursor-connections.git"
  },
  "keywords": [
    "graphql",
    "graphql-connections",
    "nestjs",
    "pagination"
  ],
  "author": "Alex Zhulin",
  "license": "MIT",
  "homepage": "https://github.com/azhulin/nestjs-graphql-cursor-connections",
  "bugs": {
    "url": "https://github.com/azhulin/nestjs-graphql-cursor-connections/issues"
  },
  "dependencies": {
    "class-validator": "^0.14.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "@nestjs/graphql": "^10.0.0 || ^11.0.0 || ^12.0.0",
    "reflect-metadata": "^0.1.0 || ^0.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "typescript": "^5.1.3"
  }
}
